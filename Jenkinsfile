pipeline {
  agent any
  stages {
    stage('Build ') {
      steps {
        echo 'Hello word '
      }
    }

    stage('Test') {
      steps {
        echo 'Running Test1'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploy the code has been succefully'
      }
    }
     stage('DeployTwo') {
      steps {
        echo 'Deployement completed'
      }
    }

  }
}